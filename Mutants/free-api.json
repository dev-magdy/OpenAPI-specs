        "description": "Returns a list of available API categories.",
        "operationId": "getCategories",
        "responses": {
          "200": {
            "description": ""
          }
        }
      }
    },
    "/health": {
      "get": {
        "tags": ["Health"],
        "summary": "/health",
        "description": "Check the status of the API.",
        "operationId": "health",
        "responses": {
