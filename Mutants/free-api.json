          },
          {
            "name": "https",
            "in": "query",
            "schema": {
              "type": "string",
              "example": ""
            },
            "description": "return entries that support HTTPS or not"
          },
          {
            "name": "cors",
            "in": "query",
            "schema": {
              "type": "string",
              "example": ""
            },
            "description": "CORS support for entry (\"yes\", \"no\", or \"unknown\")"
          },
          {
            "name": "category",
            "in": "query",
            "schema": {
              "type": "string",
              "example": ""
            },
            "description": "return entries of a specific category"
          }
        ],
        "responses": {
          "200": {
            "description": ""
          }
        }
