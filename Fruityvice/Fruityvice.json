{
  "openapi": "3.0.3",
  "info": {
    "title": "Fruityvice - OpenAPI 3.0",
    "description": "A powerful webservice which provides data for all kinds of fruit! You can use Fruityvice to find out interesting information about fruit and educate yourself. The webservice is completely free to use and contribute to.",
    "version": "1.0.1"
  },
  "externalDocs": {
    "description": "Find out more about Fruityvice",
    "url": "https://fruityvice.com/doc/index.html"
  },
  "servers": [
    {
      "url": "https://fruityvice.com/api"
    }
  ],
  "tags": [
    {
      "name": "fruit",
      "description": "Everything about fruits",
      "externalDocs": {
        "description": "Find out more",
        "url": "https://fruityvice.com/doc/index.html"
      }
    }
  ],
  "paths": {
    "/fruit/all": {
      "get": {
        "tags": [
          "fruit"
        ],
        "summary": "Gets list of all fruits in the database",
        "description": "Gets list of all fruits in the database",
        "operationId": "getAllFruits",
        "responses": {
          "200": {
            "description": "successful operation",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/Fruit"
                  }
                }
              }
            }
          },
          "500": {
            "description": "Internal server error",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "error": {
                      "type": "string",
                      "example": "Internal server error"
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    "/fruit/{nutrition}": {
      "get": {
        "tags": [
          "fruit"
        ],
        "summary": "Searches database for fruits satisfying given conditions",
        "description": "Get fruits with given nutrition between min and max parameters. At least the nutrition parameter is required.",
        "operationId": "findFruitsByNutrition",
        "parameters": [
          {
            "name": "nutrition",
            "in": "path",
            "description": "a nutrition to filter fruits by",
            "required": true,
            "schema": {
              "type": "string",
              "enum": [
                "carbohydrates",
                "protein",
                "fat",
                "calories",
                "sugar"
              ]
            }
          },
          {
            "name": "min",
            "in": "query",
            "description": "Optional minimum nutrition value [default:0]",
            "required": false,
            "schema": {
              "type": "number"
            }
          },
          {
            "name": "max",
            "in": "query",
            "description": "Optional maximum nutrition value [default:100]",
            "required": false,
            "schema": {
              "type": "number"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "successful operation",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/Fruit"
                  }
                }
              }
            }
          },
          "400": {
            "description": "Missing required parameters",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "error": {
                      "type": "string",
                      "example": "You need to provide further parameter for the request to work!"
                    }
                  }
                }
              }
            }
          },
          "404": {
            "description": "Not found",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "error": {
                      "type": "string",
                      "example": "The fruit was not found"
                    }
                  }
                }
              }
            }
          },
          "500": {
            "description": "Internal server error",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "error": {
                      "type": "string",
                      "example": "Internal server error"
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
